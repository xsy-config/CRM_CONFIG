      "module_name_(.*)": "<rootDir>/substituted_module_$1.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "promise",
      "source-map",
      "react",
      "react-native",
      "node_modules/react",
      "node_modules/react-native"
    ],
    "globals": {
      "__DEV__": true
